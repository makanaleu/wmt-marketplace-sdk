{
  "name": "wmt-marketplace-sdk-canada",
  "version": "1.3.5",
  "description": "Walmart Marketplace API SDK (Canada Marketplace)",
  "main": "index.js",
  "directories": {
    "lib": "./lib",
    "doc": "./doc",
    "example": "./example",
    "test": "./test"
  },
  "types": "./index",
  "scripts": {
    "build": "rimraf ./lib/ && tsc -p .",
    "readme": "rm ./README.md && node ./node_modules/.bin/node-readme",
    "package": "npm run build && npm run readme",
    "test": "mocha --require ts-node/register $(find ./test/ -name \"*.spec.ts\")"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/makanaleu/wmt-marketplace-sdk-canada.git"
  },
  "keywords": [
    "Walmart",
    "API",
    "SDK",
    "marketplace",
    "feeds",
    "items",
    "prices",
    "promotions",
    "orders",
    "inventory",
    "reports"
  ],
  "author": "Kane McConnell <kane@makanal.eu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makanaleu/wmt-marketplace-sdk-canada/issues"
  },
  "homepage": "https://github.com/makanaleu/wmt-marketplace-sdk-canada#readme",
  "devDependencies": {
    "ts-node": "5.0.1",
    "typescript": "2.8.1",
    "tslint": "5.9.1",
    "mocha": "5.1.0",
    "nock": "9.2.5",
    "chai": "4.1.2",
    "node-readme": "0.1.9",
    "nyc": "11.6.0",
    "rimraf": "2.6.2",
    "@types/mocha": "5.0.0",
    "@types/nock": "9.1.3",
    "@types/chai": "4.1.2",
    "@types/node": "9.6.5",
    "@types/request-promise": "4.1.41",
    "@types/bluebird": "3.5.20"
  },
  "dependencies": {
    "wmt-marketplace-auth": "1.1.0",
    "request": "2.85.0",
    "request-promise": "4.2.2"
  }
}
